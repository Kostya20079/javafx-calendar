<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="486.0" prefWidth="669.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.calendar.CalendarController">
   <children>
      <Label fx:id="currentDateLabel" text="currentDate">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button fx:id="switchWeekAhead" mnemonicParsing="false" onAction="#setWeekAhead" text="Switch a week ahead" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
